{
	"name": "@qoslab/shared",
	"version": "0.1.0",
	"exports": "./deno.ts",
	"fmt": {
		"semiColons": false
	},
	"compilerOptions": {
		"lib": ["ESNext", "DOM"],
		"jsx": "react-jsx",
		"verbatimModuleSyntax": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noPropertyAccessFromIndexSignature": false
	}
}
